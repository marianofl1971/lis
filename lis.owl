@prefix : <https://w3id.org/def/lis#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/def/lis> .

<https://w3id.org/def/lis> rdf:type owl:Ontology ;
                            owl:imports <http://www.w3.org/2004/02/skos/core> ;
                            <http://purl.org/dc/elements/1.1/title> "Ontology for the Lipid Identification Service (LIS)"@en ;
                            <http://purl.org/dc/terms/creator> "Mariano Fernández López"@es ;
                            <http://purl.org/dc/terms/license> "Creative Commons CC-BY-SA"@es ;
                            rdfs:comment "This is is the ontology assumed by the [Lipid Identification Service (LIS)](https://github.com/marianofl1971/lis). It defines notions like experiment, hypothesis, empirical compound, theoretical compound, etc. The examples in JSON have been validated using [JSONLint](https://jsonlint.com/), and the examples in JSON-LD, using [JSON-LD Playground](https://json-ld.org/playground/)."@en .

#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/def/lis#hasCertaintyLevel
:hasCertaintyLevel rdf:type owl:ObjectProperty ;
                   rdfs:domain :CLAwithExplanations ;
                   rdfs:range :CertaintyLevel ;
                   rdfs:comment "It relates certainty assignments with explanations to certainty levels."@en ;
                   rdfs:label "has certainty level"@en .


###  https://w3id.org/def/lis#hasEmpiricalCompound
:hasEmpiricalCompound rdf:type owl:ObjectProperty ;
                      rdfs:range :EmpiricalCompound ;
                      rdfs:comment "It is the relation between entities (e.g. hypotheses) and empirical compounds."@en ;
                      rdfs:label "has empirical compound"@en .


###  https://w3id.org/def/lis#hasExplanation
:hasExplanation rdf:type owl:ObjectProperty ;
                rdfs:range :Explanation ;
                rdfs:comment "It relates entities (e.g. certainty level assignments with explanations) to explanations."@en ;
                rdfs:label "has explanation"@en .


###  https://w3id.org/def/lis#hasHypothesis
:hasHypothesis rdf:type owl:ObjectProperty ;
               rdfs:range :Hypothesis ;
               rdfs:comment "It is the relation between entities (e.g. experiments or certainty level assignments with explanations) and hypotheses."@en ;
               rdfs:label "has hypothesis"@en .


###  https://w3id.org/def/lis#hasIonizationMode
:hasIonizationMode rdf:type owl:ObjectProperty ;
                   rdfs:domain :Experiment ;
                   rdfs:range :IonizationMode ;
                   rdfs:comment "It relates experiments with ionization modes."@en ;
                   rdfs:label "has ionization mode"@en .


###  https://w3id.org/def/lis#hasModifier
:hasModifier rdf:type owl:ObjectProperty ;
             rdfs:domain :Experiment ;
             rdfs:range :Modifier ;
             rdfs:comment "It relates experiments with modifieres."@en ;
             rdfs:label "has modifier"@en .


###  https://w3id.org/def/lis#hasRuleType
:hasRuleType rdf:type owl:ObjectProperty ;
             rdfs:domain :Explanation ;
             rdfs:range :RuleType ;
             rdfs:comment "It relates explanations to rule types."@en ;
             rdfs:label "has rule type"@en .


###  https://w3id.org/def/lis#hasTheoreticalCompound
:hasTheoreticalCompound rdf:type owl:ObjectProperty ;
                        rdfs:range :TheoreticalCompound ;
                        rdfs:comment "It is the relation between entities (e.g. hypotheses) and theoretical compounds."@en ;
                        rdfs:label "has theoretical compound"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/def/lis#additionalInformation
:additionalInformation rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :explanationContent ;
                       rdfs:domain :Explanation ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Each fact instantiating this property relates an explanation of ionization rule to its additional information."@en ;
                       rdfs:label "additional information"@en .


###  https://w3id.org/def/lis#adduct
:adduct rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:string ;
        rdfs:comment "Each fact instantiating this property relates an entity (e.g. a hypothesis) with a string representing a compound playing the role of adduct."@en ;
        rdfs:label "adduct"@en .


###  https://w3id.org/def/lis#coreInformation
:coreInformation rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :explanationContent ;
                 rdfs:domain :Explanation ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Each fact instantiating this property relates an explanation to its core information."@en ;
                 rdfs:label "core information"@en .


###  https://w3id.org/def/lis#evidenceAgainst
:evidenceAgainst rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :explanationContent ;
                 rdfs:domain :Explanation ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Each fact instantiating this property relates a retention time rule explanation to an evidence against."@en ;
                 rdfs:label "evident against"@en .


###  https://w3id.org/def/lis#evidenceFor
:evidenceFor rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :explanationContent ;
             rdfs:domain :Explanation ;
             rdfs:range xsd:string ;
             rdfs:comment "Each fact instantiating this property relates a retention time rule explanation to an evidence for."@en ;
             rdfs:label "evidence for"@en .


###  https://w3id.org/def/lis#explanationContent
:explanationContent rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Explanation ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Each fact instantiating this property relates an explanation with a string."@en ;
                    rdfs:label "explanation content"@en .


###  https://w3id.org/def/lis#identifier
:identifier rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:comment "It allows selecting an entity unequivocally."@en ;
            rdfs:label "identifier"@en .


###  https://w3id.org/def/lis#mass
:mass rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:double ;
      rdfs:comment "Each fact instantiating this property relates  an entity (e.g. an empirical compound) to its mass."@en ;
      rdfs:label "mass"@en .


###  https://w3id.org/def/lis#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      rdfs:comment "Each fact instantiating this property relates an entity (e.g. a theoretical compound) to its name."@en ;
      rdfs:label "name"@en .


###  https://w3id.org/def/lis#retentionTime
:retentionTime rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:double .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/def/lis#ARRExplanation
:ARRExplanation rdf:type owl:Class ;
                rdfs:subClassOf :Explanation ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasRuleType ;
                                  owl:hasValue :adductRelationRuleType
                                ] ;
                rdfs:comment """Explanation of the execution of an adduct relation rule. Below is presented an example of empirical compound in JSON (assuming a [particular context](https://raw.githubusercontent.com/marianofl1971/lis/master/OnToology/lis.owl/context/context.jsonld):
~~~~
  {
    \"ruleType\":\"adduct_relation_type\",
    \"trueValue\":\"expected\",
    \"hypothesis\": {
      \"identifier\":315,
      \"empiricalCompound\": {\"mass\":517.3172, \"rt\":19.4689},
      \"theoreticalCompound\": {\"identifier\":2628, \"name\":\"PC(O-14:0/2:0)\"},
      \"adduct\":\"M+Na\"
    },
    \"explanation\": {
      \"coreInformation\":\"The empirical compound 517.3172@19.4689 is expected to be the theoretical compound PC(O-14:0/2:0) with adduct M+Na, because, for example, the empirical compound 495.3352@19.4689 with adduct M+H may be the theoretical compound PC(O-14:0/2:0) as well.\"
    }
  }

~~~~
and here, an example in JSON-LD:
~~~~
{
   \"@context\":
\"https://raw.githubusercontent.com/marianofl1971/lis/master/OnToology/lis.owl/context/context.jsonld\",
 
    \"ruleType\":\"adduct_relation_type\",
    \"trueValue\":\"expected\",
    \"hypothesis\": {
      \"identifier\":315,
      \"empiricalCompound\": {\"mass\":517.3172, \"rt\":19.4689},
      \"theoreticalCompound\": {\"identifier\":2628, \"name\":\"PC(O-14:0/2:0)\"},
      \"adduct\":\"M+Na\"
    },
    \"explanation\": {
      \"coreInformation\":\"The empirical compound 517.3172@19.4689 is expected to be the theoretical compound PC(O-14:0/2:0) with adduct M+Na, because, for example, the empirical compound 495.3352@19.4689 with adduct M+H may be the theoretical compound PC(O-14:0/2:0) as well.\"
    }
}
~~~~"""@en ;
                rdfs:label "adduct relation rule explanation"@en .


###  https://w3id.org/def/lis#CLAwithExplanations
:CLAwithExplanations rdf:type owl:Class ;
                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasExplanation ;
                                       owl:someValuesFrom :Explanation
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasCertaintyLevel ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :CertaintyLevel
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasHypothesis ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :Hypothesis
                                     ] ;
                     rdfs:comment """A certainty level assignment with explanations relates an hypothesis with a certainty level and with an explanation. That is, it is a reification of a ternary relation. Below is presented an example of empirical compound in JSON (assuming a [particular context](https://raw.githubusercontent.com/marianofl1971/lis/master/OnToology/lis.owl/context/context.jsonld):
~~~~
{
    \"ruleType\":\"ionization_rule_type\",
    \"trueValue\":\"expected\",
    \"hypothesis\": {
      \"identifier\":315,
      \"empiricalCompound\": {\"mass\":517.3172, \"rt\":19.4689},
      \"theoreticalCompound\": {\"identifier\":2628, \"name\":\"PC(O-14:0/2:0)\"},
      \"adduct\":\"M+Na\"
    },
    \"explanation\": {
      \"coreInformation\":\"The empirical compound 517.3172@19.4689 (in an experiment with positive ionization and modifier NH3) is expected to be the theoretical compound PC(O-14:0/2:0) with adduct M+Na, which belongs to the hierarchy GP, GP01, GP0102\",
      \"additionalInformation\":\"The empirical compound 495.3352@19.4689 is expected to be the theoretical compound PC(O-14:0/2:0) with adduct M+H, because, for example, the empirical compound 517.3172@19.4689 with adduct M+Na may be the theoretical compound PC(O-14:0/2:0) as well.\"
    }
}
~~~~
and here, an example in JSON-LD:
~~~~
{
    \"@context\": \"https://raw.githubusercontent.com/marianofl1971/lis/master/OnToology/lis.owl/context/context.jsonld\",
    \"@type\": \"https://w3id.org/def/lis#CLAwithExplanations\",
      \"ruleType\":\"ionization_rule_type\",
    \"trueValue\":\"expected\",
    \"hypothesis\": {
      \"identifier\":315,
      \"empiricalCompound\": {\"mass\":517.3172, \"rt\":19.4689},
      \"theoreticalCompound\": {\"identifier\":2628, \"name\":\"PC(O-14:0/2:0)\"},
      \"adduct\":\"M+Na\"
    },
    \"explanation\": {
      \"coreInformation\":\"The empirical compound 517.3172@19.4689 (in an experiment with positive ionization and modifier NH3) is expected to be the theoretical compound PC(O-14:0/2:0) with adduct M+Na, which belongs to the hierarchy GP, GP01, GP0102\",
      \"additionalInformation\":\"The empirical compound 495.3352@19.4689 is expected to be the theoretical compound PC(O-14:0/2:0) with adduct M+H, because, for example, the empirical compound 517.3172@19.4689 with adduct M+Na may be the theoretical compound PC(O-14:0/2:0) as well.\"
    }
}

~~~~
LIS returns an array of certainty level assignments with explanations."""@en ;
                     rdfs:label "certainty level assignment with explanations"@en .


###  https://w3id.org/def/lis#CertaintyLevel
:CertaintyLevel rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ;
                rdfs:comment "The certainty levels can be _impossible_, _improbable_, _possible_, _expected_ or _sure_."@en ;
                rdfs:label "certainty level"@en .


###  https://w3id.org/def/lis#EmpiricalCompound
:EmpiricalCompound rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :mass ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:double
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :retentionTime ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:double
                                   ] ;
                   rdfs:comment """An empirical compound is characterised by a mass and a retention time. Below is presented an example of empirical compound in JSON (assuming a [particular context](https://raw.githubusercontent.com/marianofl1971/lis/master/OnToology/lis.owl/context/context.jsonld):
~~~~
{
   \"mass\": 425.3504,
   \"retentionTime\": 19.5463
}
~~~~
and here, an example in JSON-LD:
~~~~
{
   \"@context\": \"https://raw.githubusercontent.com/marianofl1971/lis/master/OnToology/lis.owl/context/context.jsonld\",
    \"@type\": \"https://w3id.org/def/lis#EmpiricalCompound\",
    \"mass\": 425.3504,
    \"retentionTime\": 19.5463
}
~~~~"""@en ;
                   rdfs:label "empirical compound"@en .


###  https://w3id.org/def/lis#Experiment
